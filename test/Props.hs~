
{-# LANGUAGE FlexibleInstances #-}
{-# LANGUAGE TemplateHaskell #-}
module Spec where

import Test.QuickCheck
import Data.Array.Repa (Array, DIM1, DIM2, DIM3, U, D, Z (..), (:.)(..),
                        (!), (++), (*^), (+^), (-^), (/^))
prop_a = 1 == 1

main :: IO Bool
main = check

return []
check = $quickCheckAll
